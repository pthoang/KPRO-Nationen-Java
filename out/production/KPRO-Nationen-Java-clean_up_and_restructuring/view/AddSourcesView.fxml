   </padding>
   <children>
            <Label GridPane.columnIndex="0" GridPane.rowIndex="0" text="Skatteetaten" />
            <TextField fx:id="loadedFileNameField" GridPane.columnIndex="1" GridPane.rowIndex="0"/>
            <Button fx:id="chooseFileButton" GridPane.columnIndex="2" GridPane.rowIndex="0" mnemonicParsing="false" onAction="#fileChooser" text="Last inn" />

            <Label GridPane.columnIndex="0" GridPane.rowIndex="1" text="AksjonÃ¦rregisteret" />
            <TextField fx:id="loadedFileNameField1" GridPane.columnIndex="1" GridPane.rowIndex="1" />
