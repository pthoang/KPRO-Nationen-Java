<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.AnchorPane?>

<GridPane xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controllers.ListCandidatesController">
<children>
    <TableView fx:id="candidateTable" GridPane.hgrow="ALWAYS" GridPane.vgrow="ALWAYS" GridPane.columnIndex="0" GridPane.rowIndex="0">
        <columnResizePolicy><TableView fx:constant="CONSTRAINED_RESIZE_POLICY"/></columnResizePolicy>
        <columns>
            <TableColumn fx:id="rankColumn" prefWidth="50.0" text="Rang" />
            <TableColumn fx:id="lastNameColumn" prefWidth="50.0" text="Etternavn" />
            <TableColumn fx:id="firstNameColumn" prefWidth="50.0" text="Fornavn" />
        </columns>
    </TableView>
    <ToolBar GridPane.columnIndex="0" GridPane.rowIndex="1">
        <items>
            <Button fx:id="backButton" mnemonicParsing="false" onAction="#handleBack" text="Tilbake" />
            <Button fx:id="nextButton" mnemonicParsing="false" onAction="#handleNext" text="Lagre" />
        </items>
    </ToolBar>
</children>
</GridPane>