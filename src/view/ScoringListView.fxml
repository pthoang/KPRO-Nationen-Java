<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.BorderPane?>

<GridPane xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controllers.ScoringListController">
   <children>
       <TableView fx:id="candidateTable" GridPane.columnIndex="0" GridPane.rowIndex="0">
            <columns>
                <TableColumn fx:id="rankColumn" text="Rang" />
                <TableColumn fx:id="lastNameColumn" text="Etternavn" />
                <TableColumn fx:id="firstNameColumn" text="Fornavn" />
            </columns>
       </TableView>
       <fx:include fx:id="candidate" source="CandidateView.fxml" />
       <ToolBar GridPane.columnIndex="1" GridPane.rowIndex="0">
            <items>
                <Button fx:id="backButton" mnemonicParsing="false" onAction="#handleBack" text="Tilbake" />
                <Button fx:id="saveButton" mnemonicParsing="false" onAction="#handleSave" text="Lagre" />
            </items>
       </ToolBar>
   </children>
</GridPane>
